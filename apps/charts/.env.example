# Auth.js Secret (Generate with: openssl rand -base64 32)
AUTH_SECRET=your-secret-key-here

# Database URLs (for local development)
DATABASE_URL_USERS="file:../../packages/database-users/prisma/dev.db"
DATABASE_URL_CHARTS="file:../../packages/database-charts/prisma/dev.db"

# OpenRouter API Key (Get from https://openrouter.ai/keys)
OPENROUTER_API_KEY=your-openrouter-api-key

# Cloudflare D1 (for production - configured in wrangler.toml)
# DB_USERS=<bound via wrangler>
# DB_CHARTS=<bound via wrangler>
