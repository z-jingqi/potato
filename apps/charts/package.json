    "@potato/ui": "workspace:*",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.475.0",
    "next": "^15.4.5",
    "next-auth": "^4",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.11.1",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:^",
